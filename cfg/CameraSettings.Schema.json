{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "img_width": {"type": "integer"},
      "img_height": {"type": "integer"},
      "fps": {"type": "number"},
      "pixel_format": {
        "enum": ["BayerGB8", "YCbCr422_8_CbYCrY", "RGB8"]
      },
      "video_mode": {"type": "string"},
      "black_level": {"type": "number"},
      "gain": {"type": "number"},
      "exposure_time": {"type": "number"},
      "binning_vertical": {"type": "integer"},
      "save_dir": {"type": "string"},
      "codecName": {"type": "string"}
    },
    "required": [
      "img_width", "img_height", "fps", "pixel_format", "video_mode",
      "black_level", "gain", "exposure_time", "binning_vertical",
      "save_dir"
    ]
  }
  